#Use this file to create synthetic data!

user<- "shannon"
countryName<- 06
version<- "v3"
censusYear<- 2010 #dummy variable currently
geoLevel<- 2
geoID<- "06"
isUSA<- TRUE
samplingMethod<- "unif"
# this is just a quickfix for when I'm running it... will clean up 
if(user == "lee2"){
  inputFolder<- "C:/Users/leeri_000/midas_v2/"
} else if(user == "lee"){
  inputFolder <- "C:/Users/Lee/midas_v2/"
} else {
  inputFolder<- "~/midas_v2/"
}

## Call a single county 
source(paste0(inputFolder, "/mainRun/main_", version,".R"))
ptm <- proc.time()
mainRun(countryName, censusYear, geoLevel, version, user, inputFolder, isUSA, geoID, samplingMethod)
print(proc.time() - ptm)


